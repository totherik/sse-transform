{
  "name": "sse-transform",
  "version": "1.0.0",
  "description": "A transform stream for converting JavaScript objects to Server-Sent Events.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*.js",
    "cover": "istanbul cover tape -- test/*.js",
    "lint": "eslint index.js lib/*.js",
    "dryrun": "(TARBALL=`npm pack` && tar ztvf $TARBALL && rm -f $TARBALL)"
  },
  "keywords": [
    "server",
    "sent",
    "event",
    "stream"
  ],
  "author": "Erik Toth <totherik@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^0.8.2",
    "istanbul": "^0.3.2",
    "tape": "^3.0.0"
  },
  "dependencies": {
    "through2": "^0.6.2"
  }
}
